{"file":"index.vue.mjs","mappings":";;;;;;;;;AAIA,UAAM,QAAQ,IAAI,EAAE;;;;;;;;;;;;;;;;;","names":[],"sources":["../../../../../../../src/pages/admin/users/index.vue"],"sourcesContent":["<!-- src/pages/admin/users/index.vue -->\n<script setup>\nimport { ref, onMounted } from 'vue';\n\nconst users = ref([]);\n\n// Fetch users from API or mock data\nconst fetchUsers = async () => {\n  try {\n    // Simulate an API call or use an actual API to fetch users\n    users.value = [\n      { id: 1, name: 'John Doe', email: 'john.doe@example.com', role: 'Admin', status: 'Active' },\n      { id: 2, name: 'Jane Smith', email: 'jane.smith@example.com', role: 'User', status: 'Inactive' },\n    ];\n  } catch (error) {\n    console.error('Failed to fetch users:', error);\n  }\n};\n\nonMounted(() => {\n  fetchUsers();\n});\n\nconst editUser = (userId) => {\n  // Navigate to the edit user page\n  navigateTo(`/admin/users/${userId}`);\n};\n\nconst resetPassword = (userId) => {\n  // Reset the password for the user\n  alert(`Password reset for user ID: ${userId}`);\n};\n\nconst toggleUserStatus = (userId) => {\n  // Toggle the user account status (active/inactive)\n  alert(`Toggling status for user ID: ${userId}`);\n};\n</script>\n\n<template>\n  <div>\n    <h2>User Management</h2>\n    <table>\n      <thead>\n        <tr>\n          <th>Name</th>\n          <th>Email</th>\n          <th>Role</th>\n          <th>Status</th>\n          <th>Actions</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"user in users\" :key=\"user.id\">\n          <td>{{ user.name }}</td>\n          <td>{{ user.email }}</td>\n          <td>{{ user.role }}</td>\n          <td>{{ user.status }}</td>\n          <td>\n            <button @click=\"editUser(user.id)\">Edit</button>\n            <button @click=\"resetPassword(user.id)\">Reset Password</button>\n            <button @click=\"toggleUserStatus(user.id)\">Toggle Status</button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<style scoped>\ntable {\n  width: 100%;\n  border-collapse: collapse;\n}\n\ntable th, table td {\n  padding: 10px;\n  border: 1px solid #ddd;\n}\n\nbutton {\n  margin-right: 5px;\n  padding: 5px 10px;\n  background-color: #007bff;\n  color: white;\n  border: none;\n  cursor: pointer;\n}\n\nbutton:hover {\n  background-color: #0056b3;\n}\n</style>\n"],"version":3}